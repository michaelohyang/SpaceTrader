<AnchorPane prefHeight="734.0" prefWidth="1033.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EncounterTraderController">
   <children>
      <AnchorPane prefHeight="734.0" prefWidth="1033.0">
         <children>
            <ImageView fitHeight="734.0" fitWidth="1033.0" pickOnBounds="true">
               <image>
                  <Image url="@../pictures/TraderEncounterPic.png" />
               </image>
            </ImageView>
            <TextField alignment="CENTER" editable="false" layoutX="215.0" layoutY="25.0" prefHeight="119.0" prefWidth="604.0" promptText="Meet The Trader" text="You've Encountered A Trader">
               <font>
                  <Font name="Bauhaus 93" size="40.0" />
               </font>
               <effect>
                  <SepiaTone />
               </effect>
            </TextField>
            <Button fx:id="aButton" layoutX="432.0" layoutY="398.0" mnemonicParsing="false" onAction="#toTraderScreen" prefHeight="70.0" prefWidth="170.0" text="Meet The Trader">
               <font>
                  <Font name="Bauhaus 93" size="16.0" />
               </font>
               <effect>
                  <SepiaTone />
               </effect>
            </Button>
            <Button fx:id="backBtn" layoutX="385.0" layoutY="524.0" mnemonicParsing="false" onAction="#toGUIScreen" prefHeight="70.0" prefWidth="264.0" text="Proceed Onto Your Journey">
