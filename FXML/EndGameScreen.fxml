<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="777.0" prefWidth="964.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EndGameController">
   <children>
      <ImageView fitHeight="777.0" fitWidth="964.0" pickOnBounds="true">
         <image>
            <Image url="@../pictures/EndSpaceship.jpg" />
         </image>
      </ImageView>
      <Label layoutX="37.0" layoutY="41.0" prefHeight="49.0" prefWidth="880.0" text="IT WAS A GOOD JOURNEY WITH YOU CAPTAIN!" textFill="WHITE">
         <font>
            <Font name="Bell MT" size="33.0" />
         </font>
      </Label>
      <Label layoutX="37.0" layoutY="110.0" text="WE WISH TO TRAVEL WITH YOU AGAIN! " textFill="WHITE">
         <font>
            <Font name="Bell MT" size="33.0" />
         </font>
      </Label>
      <Button fx:id="restartButton" layoutX="370.0" layoutY="178.0" mnemonicParsing="false" onAction="#restartGame" prefHeight="36.0" prefWidth="141.0" text="RESTART JOURNEY" textAlignment="CENTER">
         <effect>
            <SepiaTone />
         </effect>
         <font>
            <Font name="Bauhaus 93" size="12.0" />
         </font>
      </Button>
      <Label layoutX="37.0" layoutY="176.0" prefHeight="39.0" prefWidth="257.0" text="FAREWELL SIR!" textFill="WHITE">
         <font>
            <Font name="Bell MT" size="33.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
